{
  "name": "@allors/base-promise",
  "version": "2.0.0",
  "description": "Allors Base Promise",
  "repository": "https://github.com/Allors/allors2",
  "license": "LGPL-3.0",
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "lint": "tslint --project ./tsconfig.json",
    "lint-fix": "tslint --fix --project ./tsconfig.json",
    "test": "mocha --timeout 30000 --require ts-node/register --preserve-symlinks ./test/**/*.spec.ts",
    "tc:test": "mocha --timeout 30000 --require ts-node/register --preserve-symlinks --reporter mocha-teamcity-reporter ./test/**/*.spec.ts",
    "az:test": "mocha --timeout 30000 --require ts-node/register --preserve-symlinks --reporter mocha-trx-reporter ./test/**/*.spec.ts "
  },
  "dependencies": {
    "axios": "latest"
  },
  "devDependencies": {
    "@types/chai": "latest",
    "@types/lodash": "latest",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "chai": "latest",
    "lodash": "latest",
    "mocha": "latest",
    "mocha-teamcity-reporter": "latest",
    "mocha-trx-reporter": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "lnk-cli": "latest"
  }
}
