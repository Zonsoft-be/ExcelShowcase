<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="coreTab" label="Core">
        <group id="generalGroup" label="General">
          <labelControl id="userLabel" getLabel="GetUserLabel" />
          <button id="authentication" getLabel="GetAuthenticateLabel" onAction="OnAuthenticate"/>
          <button id="refresh" onAction="OnRefresh" label="Refresh" />
        </group>
        <group id="salesGroup" label="Sales">
          <button id="people" onAction="OnClick" label="People" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

