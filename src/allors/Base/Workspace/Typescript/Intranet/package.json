{
  "name": "intranet",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node ./postinstall.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "tslint --fix --project ./src/tsconfig.app.json",
    "build-stats": "ng build --aot --optimization --stats-json",
    "stats": "webpack-bundle-analyzer dist/intranet/stats-es2015.json",
    "autotest": "npx -p typescript tsc --project ./autotest/tsconfig.autotest.json && node ./out-tsc/autotest/autotest/main  && npx rimraf ./out-tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.1.6",
    "@angular/compiler": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/forms": "~9.1.6",
    "@angular/material": "^9.2.3",
    "@angular/material-moment-adapter": "~9.2.3",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "angular-calendar": "^0.28.13",
    "bootstrap": "^4.4.1",
    "common-tags": "^1.8.0",
    "date-fns": "^2.13.0",
    "easymde": "^2.10.1",
    "jsnlog": "^2.29.0",
    "moment": "2.25.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "~9.1.5",
    "@angular/compiler-cli": "~9.1.6",
    "@types/codemirror": "0.0.93",
    "@types/common-tags": "^1.8.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^0.7.4",
    "@types/node": "^12.11.1",
    "browser-env": "^3.3.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-trx-reporter": "^0.4.3",
    "lnk": "^1.1.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
