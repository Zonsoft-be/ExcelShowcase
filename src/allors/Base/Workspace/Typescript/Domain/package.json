{
  "name": "@allors/base-domain",
  "version": "2.0.0",
  "description": "Allors Base Domain",
  "repository": "https://github.com/Allors/allors2",
  "license": "LGPL-3.0",
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "lint": "tslint --fix --project ./tsconfig.json",
    "lint-fix": "tslint --fix --project ./tsconfig.json",
    "test": "mocha --require ts-node/register --preserve-symlinks ./test/**/*.spec.ts",
    "tc:test": "mocha --require ts-node/register --preserve-symlinks --reporter mocha-teamcity-reporter ./test/**/*.spec.ts",
    "az:test": "mocha --require ts-node/register --preserve-symlinks --reporter mocha-trx-reporter ./test/**/*.spec.ts"
  },
  "dependencies": {
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@types/chai": "latest",
    "@types/common-tags": "^1.8.0",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "chai": "latest",
    "lnk": "latest",
    "mocha": "latest",
    "mocha-teamcity-reporter": "latest",
    "mocha-trx-reporter": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest"
  }
}
