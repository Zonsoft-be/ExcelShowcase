<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="tabExcelShowCase" label="Showcase">
        <group id="MainMenu" label="Menu">
          <button id="AddProductSheet" onAction="OnClick"  label="Add Product Sheet" imageMso="ViewGridlinesWord" size="large" getEnabled="GetEnabled"/>
          <button id="ListCovid19Sheet" onAction="OnClick"  label="Covid 19" imageMso="ViewGridlinesWord" size="large" getEnabled="GetEnabled"/>
        </group>
        <group id="Demos" label="Demos">
          <button id="AddDemoSheet" onAction="OnClick"  label="Add Demo Sheet" imageMso="ViewGridlinesWord" size="large" getEnabled="GetEnabled"/>
          <button id="InsertPicture" onAction="OnClick"  label="Insert Image" imageMso="PictureInsertFromFile" size="large" getEnabled="GetEnabled"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

